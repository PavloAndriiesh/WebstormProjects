<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mobidev.ishop" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <name>IShop</name>
    <description>
        IShop application developed with help of RAD.js, Apache Cordova
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Pavlo Andriiesh, MobiDev
    </author>
    <icon src="icon.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
